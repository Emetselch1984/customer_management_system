<% @title = "アカウント情報編集" %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_with model: @staff, url: confirm_staff_account_url do |f| %>
    <%= render "form", f: f, confirming: false %>
    <div class="buttons">
      <%= f.submit "編集する" %>
      <%= link_to "キャンセル", staff_account_path %>
    </div>
  <% end %>
</div>
